<Page x:Class="DOT_Logs.ViewLogPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DOT_Logs"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Student Log"
      VerticalAlignment="Stretch"
      HorizontalAlignment="Stretch">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Resources/Background-2.jpg"/>
        </Grid.Background>
        <Button x:Name="backButton" Content="" HorizontalAlignment="Left" Margin="33,45,0,0" VerticalAlignment="Top" Width="49" Height="43.75" IsCancel="True" Click="backButtonClicked" Opacity="100" BorderThickness="0">
            <Button.OpacityMask>
                <ImageBrush ImageSource="Resources/BackArrow.png"/>
            </Button.OpacityMask>
            <Button.Background>
                <ImageBrush ImageSource="Resources/BackArrow.png" Stretch="Uniform"/>
            </Button.Background>
        </Button>
        <DataGrid x:Name="dataGrid" Background="Transparent" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,150,0,0">
        </DataGrid>

    </Grid>
</Page>
